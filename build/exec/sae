#!/bin/sh

case `uname -s` in            
    OpenBSD|FreeBSD|NetBSD)   
        DIR="`grealpath $0`"
        ;;                    
                              
    *)                        
        DIR="`realpath $0`" 
        ;;                    
esac                          

export LD_LIBRARY_PATH="`dirname "$DIR"`/"sae_app":$LD_LIBRARY_PATH"
"`dirname "$DIR"`"/"sae_app/sae.so" "$@"
