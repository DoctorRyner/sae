{
  "name": "sae",
  "version": "0.0.2",
  "repository": "https://github.com/DoctorRyner/sae.git",
  "author": "DoctorRyner <drryner@icloud.com>",
  "license": "MIT",
  "bin": "build/exec/index.js",
  "pkg": {
    "assets": [
      "./node_modules/glob",
      "./node_modules/js-yaml"
    ]
  },
  "scripts": {
    "build": "idris2 --build sae.ipkg",
    "release": "yarn build && idris2 src/Main.idr --codegen node -p contrib -o index.js",
    "mk-exe": "yarn release && pkg . --target nodev14-macos-x64,nodev14-linux-x64",
    "install-mac": "yarn mk-exe && mv sae-macos /usr/local/bin/sae",
    "install-linux": "yarn mk-exe && sudo mv sae-linux /usr/local/bin/sae"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "js-yaml": "^4.0.0"
  },
  "devDependencies": {
    "pkg": "^4.4.9"
  }
}
